<script>
function resizeAdv_headlines(){
var items_prop =$('.tile_cent .sausage-list-item:not(:first-child)');
if($('.new_adv_headlines').height()>10){
var cur = $('.new_adv_headlines').offset().top ;
}else{
cur=99999;
}
for (i = 1 ; i < items_prop.length; ++i){
if ($(items_prop[i]).offset().top+ $(items_prop[i]).height()+20 > cur ){
items_prop[i].style.visibility="hidden";
}else{
items_prop[i].style.visibility="visible";
}
}
}
mq.register( {
large: function() {
$('.tile_cent.b-white.sausage ').css("min-height",'');
resizeAdv_headlines()
},
small: function() {
$('.tile_cent.b-white.sausage ').css("min-height", $('.tile_cent .sausage-list-item:first-child').height()+$('.sausage-header.no-hover.sausage-header-icon.icon-maintheme.no-border').height() +30);
resizeAdv_headlines()
}
});
set_resizeblock_handler('.new_adv_headlines', function () {
resizeAdv_headlines()
});
</script>


