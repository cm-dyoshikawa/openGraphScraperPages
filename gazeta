$('.b-navbar-search-btn').click(function(){
if($('.b-navbar-search').hasClass('active')){
$('.b-navbar-search').removeClass('active');
$('.b-navbar-main').find('.item[active=1]').removeAttr('active').addClass('active');
} else {
$('.b-navbar-main').find('.item.active').attr('active', 1).removeClass('active');
$('.b-navbar-search').addClass('active');
setTimeout(function(){
$('form').find('#place_for_searchinput').focus()
}, 100);
}
})
