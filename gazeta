<script>
function showHideOnlinesBoard(){
var $slideBtn = $('.onlineboard-slide-btn'),
$onlineBoard = $('.onlineboard'),
$onlineBoardGarret = $('.onlineboard-garret');
if($slideBtn.hasClass('show')){
$onlineBoard.find('.hide-el').css('display', 'inline-block');
$slideBtn
.removeClass('show')
.html('Свернуть');
} else {
$onlineBoard.find('.hide-el').css('display', 'none');
$slideBtn
.addClass('show')
.html('Развернуть');
}
}
$('.onlineboard-garret').click(function(){
showHideOnlinesBoard();
})
</script>
