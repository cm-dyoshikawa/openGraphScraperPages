<div class="sausage-footer sausage-footer-hournews">
<div class="leftarr"></div>
<div class="rightarr"></div>
<div class="readmore">
<a href="/web/20170913081049/http://www.gazeta.ru/news/lastnews/" class="readmore-link">
Далее
</a>
</div>
<div class="info">
<span class="info-current"></span>/<span class="info-length"></span>
</div>
</div>
<div class="cf"></div>
<script>
var $hournewsList = $('.sausage-list-hournews'),
itemsQuantity = $('.sausage-list-hournews .sausage-list-item').length,
$hournewsFooter = $('.sausage-footer-hournews'),
$leftarr = $hournewsFooter.find('.leftarr'),
$rightarr = $hournewsFooter.find('.rightarr'),
$info = $hournewsFooter.find('.info'),
$infoCurrent = $info.find('.info-current'),
$infoLength = $info.find('.info-length'),
infoLengthVal = itemsQuantity - 1 // последний блок не содержит новость,
$readmore = $hournewsFooter.find('.readmore'),
curPos = 1;
$infoCurrent.html(curPos);
$infoLength.html(infoLengthVal);
function changeSlide(step){
var newPos = curPos+step;
if(newPos==itemsQuantity){
$rightarr.hide();
$info.hide();
$readmore.show();
} else if(newPos==1){
$leftarr.hide();
} else {
$leftarr.show();
$rightarr.show();
$info.show();
$readmore.hide();
}
$hournewsList.find('.sausage-list-item.active').removeClass('active');
$hournewsList.find('.sausage-list-item[data-slide='+newPos+']').addClass('active');
$infoCurrent.html(newPos);
curPos=newPos;
}
$rightarr.click(function(){
changeSlide(+1);
})
$leftarr.click(function(){
changeSlide(-1);
})
resize_news();
</script>








 </section>   

<div class="mb20"></div>
