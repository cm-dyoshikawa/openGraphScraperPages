<script>
function change_word(count, xid){
if (count>0){
var word = '<span><div class="iev">'+count+'</div></span>';
if(count % 100 <= 4 || count % 100 > 20){
if (count % 10 == 1){ word = '<span><div class="ii">'+count+'</div></span>';}
if (count % 10 > 1 && count % 10 < 5) word = '<span><div class="ija">'+count+'</div></span>';
}
}else{
word = '<div class="empty_comment"></div>';
}
var count_selector = document.querySelectorAll('rambler-comments-count[xid="'+xid+'"]');
if (count_selector.length > 1) {
for(var i=0; i<count_selector.length; i++){
count_selector[i].innerHTML = word;
}
}else{
document.querySelector('rambler-comments-count[xid="'+xid+'"]').innerHTML = word;
}
}
function rambler_update () {

var array_tr=document.getElementsByTagName('rambler-comments-count');
var array_eTc = [];
for(var i=0; i < array_tr.length; i++){
if(array_tr[i].textContent == ""){
array_eTc.push(array_tr[i]);
}
}
if(array_eTc.length > 0){
var url = "https://web.archive.org/web/20170913081049/https://c.rambler.ru/api/app/3/comments-count";
for(var i = 0; i < array_eTc.length; i++){
var x = i > 0 ? '&' : '?';
url += x + 'xid=' + array_eTc[i].attributes['xid'].value;
}
$.ajax({
url: url
})
.done(function(data){
for( key in data.xids){
change_word(data.xids[key], key);
}
})
}

setTimeout(function(){
if($('rambler-comments-count div.empty_comment')){
clean_mm_addition();
}
}, 100);
}
rambler_update ();
</script>
