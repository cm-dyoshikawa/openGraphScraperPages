<script>
var elements_for_ga = document.querySelectorAll('.maintheme-flow li')
var gaSelector_onlyClick = function(selectors,name,labels){
//Проверка что в селекторе! если строка с классами, то объединяем в массив и запускаем создание.
//Если уже объект jQuery, то сразу прогоняем его
if (typeof(selectors) == 'undefined'){
return;
}
if (typeof(selectors) == 'string'){
var strings=selectors.split(',');
for (j=0;j<strings.length;++j){
gaSelector($(strings[j].replace(' ','')), name, labels);
}
}else {
if(typeof(selectors) == 'object'){
gaElement(selectors, name, labels);
}else{
console.error('Ошибка! В качестве селектора передайте строку или объект jQuery!')
}
}
}
var gaElement = function(e,name,labels){
var element = {
is_load : e.is_load || false,
is_click : e.is_click || false,
labels : labels || ['Загружен ', 'Клик'],
send_load : send_load || null,
onClick_init : onClick_init ||null,
selector : e,
name : name || 'Test_Name',
init : init,
};
return element.init();
function init(){
this.send_load();
this.onClick_init(this);
return element;
}
//Function for Init
function onClick_init(that){
$(that.selector).find('a').on('click',function(){
if (!that.is_click){
ga('send', 'event', that.name, 'click', that.labels[1]);
that.is_click = !that.is_click;
}
});
}
//Function for Google Analytic
function send_load(){
if (!this.is_load){
ga('send', 'event', this.name, 'load', this.labels[0]);
this.is_load = !this.is_load;
}
}
}
</script>

<div class="buttons_more pu20 pd20">
