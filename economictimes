        					        ru = acurl.replace(etm, et) + ls;
        					    } else if (acurl.indexOf("//" + et) >= 0 && acurl.split(".com")[1] != lp && !multipublish) {
            						ru = acurl + ls;
            					} else if (mpu.indexOf("//" + et) >= 0  && mpu.split(".com")[1] != lp && multipublish) {
            						ru = mpu + ls;
            					}
        					}
        					
        					if(ru != '') {
        					    if(typeof localStorage != 'undefined') {
        						    localStorage.setItem('pageRedirectToCan', window.location.href + ' - ' + document.referrer);
        						}
        						_cookie.set("article_" + artId, "1", 180, "/");
        						document.location.href = ru
        					}
    					})();
    					
    					/* ========== Ends Article redirection to actual URL ============ */
    					
    					
    					
    					
    					/* Handle versions for dyanmically loading scripts while change in script */
        					objJsVer = {
                                'js_comments_v': 107,
                                'js_articleshowsp_slider_v': 9
                            }
